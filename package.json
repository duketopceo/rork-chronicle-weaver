{
  "name": "chronicle-weaver",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "description": "A historical role-playing game with AI-powered narrative generation",
  "author": "Rork <duketopceo@gmail.com>",
  "license": "Private",
  "scripts": {
    "start": "expo start",
    "start-web": "expo start --web",
    "start-web-dev": "DEBUG=expo* expo start --web",
    "clean": "rimraf dist web-build node_modules/.cache .expo",
    "reset": "npm run clean && rimraf node_modules package-lock.json && npm install",
    "prebuild": "npm run clean",
    "build": "expo export -p web --clear",
    "build:web": "npx expo export -p web --clear",
    "build:production": "cross-env NODE_OPTIONS=--max_old_space_size=4096 EXPO_DEBUG=1 EXPO_WEB=1 expo export -p web --clear && node scripts/post-build.js",
    "postinstall": "patch-package",
    "serve": "npx serve web-build",
    "deploy": "npm run build:production && firebase deploy --only hosting",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest -c jest.unit.config.js --passWithNoTests",
    "docs:organize": "node scripts/organize_docs.js",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hono/trpc-server": "^0.4.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
