    "eslint-plugin-react-native": "^5.0.0",
    "firebase-tools": "^14.8.0",
    "glob": "^11.0.3",
    "hermes-parser": "^0.29.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "jest-expo": "^53.0.7",
    "module-resolver": "^1.0.0",
    "patch-package": "^8.0.0",
