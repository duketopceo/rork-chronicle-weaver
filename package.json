  "license": "Private",
  "scripts": {
    "start": "bunx rork start -p 09d7jjhyh29mlo0qd2rjr --tunnel",
    "start-web": "bunx rork start -p 09d7jjhyh29mlo0qd2rjr --web --tunnel",
    "start-web-dev": "DEBUG=expo* bunx rork start -p 09d7jjhyh29mlo0qd2rjr --web --tunnel",
    "build": "expo export -p web",
    "build:web": "expo export -p web",
    "postbuild:web": "node scripts/post-build.js",
    "build:webpack": "webpack --mode production",
    "prebuild": "rimraf dist",
    "build:production": "npm run prebuild && npm run build:web",
    "serve": "firebase serve --only hosting",
    "deploy": "npm run build:production && firebase deploy --only hosting",
    "deploy:preview": "npm run build:production && firebase hosting:channel:deploy preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "prepare": "husky install || true"
  },
