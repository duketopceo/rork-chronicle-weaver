{
  "expo": {
    "name": "Chronicle Weaver",
    "slug": "chronicle-weaver",
    "version": "2.0.0",
    "scheme": "chronicle-weaver",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
