{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "jsx": "react",
    "lib": ["es2020", "dom"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler",
    "noEmit": true,
    "baseUrl": "..",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },  "include": [
    "../**/*.ts",
    "../**/*.tsx",
    "../.expo/types/**/*.ts",
    "../expo-env.d.ts",
    "../src"
  ],
  "exclude": [
    "node_modules"
  ]
}
